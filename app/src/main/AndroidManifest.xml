<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="com.example.projeto2bruna">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Projeto2Bruna"
        tools:targetApi="31">
        <activity
            android:name=".view.PhotosActivity"
            android:exported="false" />
        <activity
            android:name=".view.CommentsActivity"
            android:exported="false" />
        <activity
            android:name=".view.TodosActivity"
            android:exported="false" />
        <activity
            android:name=".view.PostsActivity"
            android:exported="false" />
        <activity
            android:name=".view.AlbumActivity"
            android:exported="false" />
        <activity
            android:name=".view.UsersActivity"
            android:exported="false" />
        <activity
            android:name=".view.MainActivity"
            android:exported="false" />
        <activity
            android:name=".view.LoginActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>